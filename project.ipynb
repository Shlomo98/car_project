   "source": [
    "# function that runs on each ad and retrieves the information about it\n",
    "def all_ads(local_url):\n",
    "    local_page = requests.get(url, headers=headers)\n",
    "    soup = BeautifulSoup(local_page.content, \"html.parser\")\n",
    "    car_ads = soup.find('div', class_=\"cards-wrap s m l\")\n",
    "    ads = car_ads.find_all('div', class_=\"card-body p-md-3\")\n",
    "    return ads"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 97,
   "id": "3a1464a5",
   "metadata": {},
   "outputs": [],
   "source": [
    "#A function that passes a table from the ad page. Goes row by row in the table and saves each value in the dictionary\n",
    "def all_tables(tables):\n",
    "    table_data = {}\n",
    "    for table in tables:\n",
    "        rows = table.find_all('tr')\n",
    "                \n",
    "        for row in rows:\n",
    "            cells = row.find_all('td')\n",
    "            if len(cells) == 2:\n",
    "                key = cells[0].get_text().strip()\n",
    "                value = cells[1].get_text().strip()\n",
    "                table_data[key] = value\n",
    "    return table_data"
   ]
  },
