    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "from datetime import datetime\n",
    "import numpy as np\n",
    "import csv\n",
    "import os\n",
    "\n",
    "\n",
    "#Retrieving the appropriate URL and starting work with the library with the main HTML\n",
    "headers={\"User-Agent\": \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Safari/537.36 OPR/110.0.0.0\"}\n",
    "url = 'https://www.ad.co.il/car?sp261=13917' \n",
    "car_ads= all_ads(url)\n",
    "response = requests.get(url)\n",
    "html_content = response.text\n",
    "soup = BeautifulSoup(html_content, 'html.parser')\n",
    "\n",
    "\n",
    "#A list that will hold the data from each ad\n",
    "ad_data=[]\n",
    "ad_response = requests.get(url)\n",
    "\n",
    "#A loop that will go through all the ads on the page\n",
    "for ad in car_ads:\n",
    "    car = ad.find('h2', class_='card-title mb-0 mb-sm-1').text\n",
    "#In addition, since most of the information is not in the ad card but in the ad itself,\n",
    "#we will attach a new link that will also loop for most of the information.\n",
    "    ad_link = ad.find('a')['href']\n",
    "    ad_url = f\"https://www.ad.co.il{ad_link}\"\n",
    "    ad_response = requests.get(ad_url)\n",
    "\n",
    "#The beginning of adding variables to the data is\n",
    "#sometimes reduced by using more internal tags to make it easier to retrieve the data   \n",
    "    if ad_response.status_code == 200:\n",
    "        ad_soup1 = BeautifulSoup(ad_response.content, 'html.parser')\n",
    "        ad_soup=ad_soup1.find('div',class_='tab-content bg-white rounded-3 border p-3 col-xxl-10 offset-xxl-1 col-md-12 offset-md-0')\n",
    "        tables = ad_soup.find_all('table', class_='table table-sm mb-4')\n",
    "        ad_table_data = all_tables(tables)\n",
    "        price=ad.find('div', class_='price ms-1').text\n",
    "        car = ad.find('h2', class_='card-title mb-0 mb-sm-1').text\n",
    "        car=car.split()\n",
    "        men=car[0]\n",
    "        model=car[1:]\n",
    "        model = ', '.join(model)\n",
    "        soup_for_pic=ad_soup.find('div',class_='col-xxl-8 col-lg-8 col-md-7')\n",
    "        picturs = soup_for_pic.find_all('img')\n",
    "        len_pic=len(picturs)\n",
    "        \n",
    "        if len(picturs)>1:\n",
    "            len_pic=len(picturs)-1\n",
    "        else :\n",
    "            len_pic=0\n",
    "        \n",
    "#\n",
    "        creation_date_element = ad_soup.find_all('div', class_='px-3')[0]\n",
    "        last_boost_date_element = ad_soup.find_all('div', class_='px-3')[1]\n",
    "\n",
    "# Extract the text from the elements\n",
    "        creation_date_text = creation_date_element.get_text()\n",
    "        last_boost_date_text = last_boost_date_element.get_text()\n",
    "\n",
    "# Extract the dates from the text\n",
    "        creation_date_str = creation_date_text.split(': ')[1]\n",
    "        last_boost_date_str = last_boost_date_text.split(': ')[1]\n",
    "\n",
    "# Parse the dates using datetime\n",
    "        creation_date = datetime.strptime(creation_date_str, '%d/%m/%Y')\n",
    "        last_boost_date = datetime.strptime(last_boost_date_str, '%d/%m/%Y')\n",
    "    \n",
    "        description=ad_soup.find_all('p', class_='text-word-break')\n",
    "        text_list = [tag.get_text() for tag in description]\n",
    "        description = ' '.join(text_list)\n",
    "        \n",
    "\n",
    "        \n",
    "#Creating a data frame and inserting the variables. \n",
    "#Carrying out a reorganization according to the entertainment requested in the assignment\n",
    "        ad_data.append({ 'יצרן':men,'דגם':model,**ad_table_data,'מחיר':price,'מספר תמונות':len_pic,'תאריך יצירה':creation_date,'תאריך הקפצה':last_boost_date,'תיאור':description})   \n",
    "\n",
    "    else :\n",
    "        print('error')\n",
    "        \n",
    "new_order = ['יצרן', 'שנה', 'דגם', 'יד', 'ת. הילוכים', 'נפח', 'סוג מנוע', 'בעלות קודמת', 'בעלות נוכחית', 'אזור', 'עיר', 'מחיר','מספר תמונות', 'תאריך יצירה', 'תאריך הקפצה', 'תיאור',  'צבע', 'ק\"מ','טסט עד',]       \n",
    "df = pd.DataFrame(ad_data)\n",
    "\n",
    "\n",
    "#Since there is no information about the test date in the ads, we added a manual column and filled in empty values\n",
    "df['טסט עד']=None\n",
    "df = df[new_order]      \n",
    "display(df)\n",
    "\n"
   ]
